    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@vercel/ncc": "^0.34.0",
    "eslint": "8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-github": "^4.6.1",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.3.1",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.3",
