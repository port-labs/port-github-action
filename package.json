    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^27.0.2",
    "@types/node": "^18.7.13",
    "@vercel/ncc": "^0.34.0",
    "eslint": "8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^26.8.7",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
